{
  "name": "svelte-bootstrap-tag-input",
  "version": "1.0.0",
  "type": "module",
  "module": "dist/svelte-bootstrap-tag-input.es.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "start": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json --ignore node_modules",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch --ignore node_modules",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . --cache --fix",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.39",
    "@types/eslint": "^8.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "bootstrap": "^5.1.3",
    "esbuild": "^0.14.25",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "husky": "^7.0.4",
    "keyboard-event-key-type": "github:ohmree/keyboard-event-key-type#fixed-intellisense",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "sass": "^1.49.9",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-preprocess": "^4.10.4",
    "svelte-preprocess-esbuild": "^2.0.0",
    "typescript": "^4.6.2",
    "typescript-svelte-plugin": "^0.3.0",
    "vite": "^2.8.6"
  },
  "peerDependencies": {
    "bootstrap": "^5.1.3",
    "svelte": "^3.0.0"
  },
  "lint-staged": {
    "*.{ts,svelte}": [
      "prettier --ignore-path .gitignore --check --plugin-search-dir=.",
      "eslint --ignore-path .gitignore --cache --fix"
    ],
    "*.{cjs,json,ts,md,svelte,html}": "prettier --ignore-path .gitignore --write --plugin-search-dir=."
  }
}
